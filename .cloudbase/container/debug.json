{
  "containers": [
    {
      "name": "wxcloudrun-express",
      "domain": "",
      "ip": ""
    }
  ],
  "config": {
    "wxcloudrun-express": {
      "containerPort": 80,
      "dockerfilePath": "Dockerfile",
      "buildDir": "",
      "minNum": 0,
      "maxNum": 1,
      "cpu": 1,
      "mem": 0.5,
      "policyType": "cpu",
      "policyThreshold": 80,
      "envParams": {},
      "customLogs": "stdout",
      "initialDelaySeconds": 2,
      "dataBaseName": "nodejs_demo",
      "executeSQLs": [
        "CREATE DATABASE IF NOT EXISTS nodejs_demo;",
        "USE nodejs_demo;"
      ]
    }
  }
}